{
  "name": "@pixi/storybook-renderer",
  "version": "0.0.1",
  "description": "Storybook PixiJS renderer",
  "homepage": "https://github.com/pixijs/storybook/tree/main/packages/storybook-renderer",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixijs/storybook.git",
    "directory": "packages/storybook-renderer"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./preview": {
      "require": "./dist/config.js",
      "import": "./dist/config.mjs",
      "types": "./dist/config.d.ts"
    },
    "./package.json": {
      "require": "./package.json",
      "import": "./package.json",
      "types": "./package.json"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "build": "../../scripts/prepare/bundle.ts"
  },
  "dependencies": {
    "@storybook/addons": "7.0.0-alpha.48",
    "@storybook/core-client": "7.0.0-alpha.48",
    "@storybook/csf": "0.0.2--canary.0899bb7.0",
    "@storybook/docs-tools": "7.0.0-alpha.48",
    "@storybook/preview-web": "7.0.0-alpha.48",
    "@storybook/store": "7.0.0-alpha.48",
    "deep-equal": "^2.0.5",
    "global": "^4.4.0",
    "pixi.js": "^6.5.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "ts-dedent": "^2.0.0"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "typescript": "~4.6.3"
  },
  "peerDependencies": {
    "@babel/core": "*"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/config.ts"
    ],
    "platform": "browser"
  }
}
